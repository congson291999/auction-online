<div class="page-wrapper">
            <!-- ============================================================== -->
            <!-- Container fluid  -->
            <!-- ============================================================== -->
            <div class="container-fluid">
                <!-- ============================================================== -->
                <!-- Bread crumb and right sidebar toggle -->
                <!-- ============================================================== -->
                <div class="row page-titles">
                    <div class="col-md-5 align-self-center">
                        <h4 class="text-themecolor"></h4>
                    </div>
                    <div class="col-md-7 align-self-center text-right">
                        <div class="d-flex justify-content-end align-items-center">
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="/admin/dashboard">Home</a></li>
                                <li class="breadcrumb-item"><a href="javascript:void(0)">User</a></li>
                                <li class="breadcrumb-item active">Seller</li>
                            </ol>
                        </div>
                    </div>
                </div>
                <!-- ============================================================== -->
                <!-- End Bread crumb and right sidebar toggle -->
                <!-- ============================================================== -->
                <!-- ============================================================== -->
                <!-- Info box Content -->
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                                <h4 class="card-title">Sellers</h4>
                                <div class="table-responsive m-t-40">
                                    <table id="myTable" class="table table-bordered table-striped">
                                        <thead>
                                            <tr>
                                                <th>ID</th>
                                                <th>Account</th>
                                                <th>Name</th>
                                                <th>Address</th>
                                                <th>Birthday</th>
                                                <th>Point</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            {{#each this.list}}
                                            <tr>
                                                <td>{{id}}</td>
                                                <td>{{email}}</td>
                                                <td>{{name}}</td>
                                                <td>{{address}}</td>
                                                <td>{{birthday}}</td>
                                                <td>{{Point}}</td>
                                            </tr>
                                            {{/each}}
                                        </tbody>
                                    </table>
                                </div>
                                <nav aria-label="" class="mt-3">
                                    <ul class="pagination">
                                        <li class="{{#if not_prev}}disabled{{/if}}">
                                        <a class="page-link text-dark" href="?page={{prev_value}}" tabindex="-1" aria-disabled="true">Previous</a>
                                        </li>
                                        {{#each this.page_numbers}}
                                        {{#if isCurrentPage}}
                                            <li class="active" aria-current="page">
                                                <a class="page-link" href="javascript:;">{{value}} <span class="sr-only">(current)</span></a>
                                            </li>
                                        {{else}}
                                            <li>
                                                <a class="page-link text-dark" href="?page={{value}}">{{value}}</a>
                                            </li>
                                        {{/if}}
                                        {{/each}}
                                        <li class="{{#if not_next}}disabled{{/if}}">
                                        <a class="page-link text-dark" href="?page={{next_value}}">Next</a>
                                        </li>
                                    </ul>
                                </nav>
                            </div>
                        </div>

                    </div>
                </div>
                <!-- ============================================================== -->

            </div>
            <!-- ============================================================== -->
            <!-- End Container fluid  -->
            <!-- ============================================================== -->
</div>