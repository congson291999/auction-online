 <div class="breadcrumb-area">
    <div class="container">
        <div class="breadcrumb-content">
            <ul>
                <li><a href="/"></a>Home</a></li>
                {{#if category}}
                <li>Categories</li>
                <li class="active">{{category.name}}</li>
                {{else}}
                    <li class="active">Categories</li>
                {{/if}}
            </ul>
        </div>
    </div>
</div>
<!-- Li's Breadcrumb Area End Here -->
<!-- Begin Li's Content Wraper Area -->
<div class="content-wraper pt-4 pb-4">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <!-- shop-top-bar start -->
                <div class="shop-top-bar">
                    <!-- product-select-box start -->
                    <div class="product-select-box">
                        <div class="product-short">
                            <p>Sort By:</p>
                            <select class="nice-select">
                                <option value="trending">Sort</option>
                                <option value="sales"><a href="http://localhost:3000/category/1?orderBy=1">Name</a></option>
                                <option value="sales"><a href="http://localhost:3000/category/1?orderBy=2">Time</a></option>
                                <option value="sales"><a href="http://localhost:3000/category/1?orderBy=3">Price</a></option>
                            </select>
                        </div>
                    </div>
                    <!-- product-select-box end -->
                </div>
                <!-- shop-top-bar end -->
                <!-- shop-products-wrapper start -->
                <div class="shop-products-wrapper">
                    <div class="tab-content">
                        <div id="grid-view" class="tab-pane fade active show" role="tabpanel">
                            <div class="product-area shop-product-area">
                                <div class="row">
                                    {{#each data}}
                                    <div class="col-lg-3 col-md-4 col-sm-6 mt-40">
                                        <!-- single-product-wrap start -->
                                        <div class="single-product-wrap">
                                            <div class="product-image">
                                                <a href="/product/{{id}}">
                                                    <img src="../images/product/{{id}}/1.jpg"
                                                        alt="Li's Product Image">
                                                </a>
                                            </div>
                                            <div class="product_desc">
                                                <div class="product_desc_info">
                                                    <div class="product-review">
                                                       
                                                       
                                                    </div>
                                                    <h4><a class="product_name" href="/product/{{id}}">
                                                            {{name}} </a></h4>
                                                    <div><span class="start-date"><span class="fa fa-calendar"></span>
                                                            {{{end_time}}}</span></div>
                                                    <div class="price-box">
                                                    
                                                        <span class="new-price new-price-2">{{price_start}}
                                                            VND</span>
                                                    </div>
                                                  
                                                </div>

                                                <div class="add-actions">
                                                    <ul class="add-actions-link">
                                                        <form method="POST">
                                                            <input id="txtId" name="id" style="display:none;"
                                                                value={{id}}></input>
                                                            <li class="add-cart active"><a href="/product/{{id}}">Add to cart</a></li>
                                                            <li><a href="#" title="quick view" class="quick-view-btn" data-toggle="modal" data-target="#exampleModalCenter"><i class="fa fa-eye"></i></a></li>
                                                            <li><button style="border:none; background:0"
                                                                    formaction="/wishlist/add" type="submit"><i
                                                                        class="fa fa-heart-o"></i></button></li>                        
                                                        </form>
                                                    </ul>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- single-product-wrap end -->
                                    </div>
                                    {{/each}}
                                </div>
                                <nav aria-label="..." class="mt-4">
                                    <ul class="pagination">
                                        <li class="page-item {{#if not_prev}}disabled{{/if}}">
                                            <a class="page-link" href="?page={{prev_value}}" tabindex="-1" aria-disabled="true">Previous</a>
                                        </li>
                                        {{#each this.page_numbers}}
                                        {{#if isCurrentPage}}
                                        <li class="page-item active" aria-current="page">
                                            <a class="page-link" href="javascript:;">{{value}}<span class="sr-only">(current)</span></a>
                                        </li>
                                        {{else}}
                                        <li class="page-item"><a class="?page={{value}}" href="#">{{value}}</a></li>
                                        {{/if}}
                                        {{/each}}
                                        <li class="page-item {{#if not_next}}disabled{{/if}}">
                                        <a class="page-link" href="?page={{next_value}}">Next</a>
                                        </li>
                                    </ul>
                                </nav>
                            </div>
                        </div>
                    </div>
                </div>




                <!-- shop-products-wrapper end -->
            </div>
        </div>
    </div>
</div>
<!-- Content Wraper Area End Here -->
<!-- #content end -->